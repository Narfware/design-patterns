{
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "@typescript-eslint/method-signature-style": "off",
        "@typescript-eslint/no-extraneous-class": "off"
    }
}